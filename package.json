{
  "name": "FastEvent",
  "version": "1.0.0",
  "description": "Manage Events, Notify Everyone",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "build": "node-sass --include-path scss scss/style.scss public/css/style.css --style nested",
    "watch-css": "nodemon -e scss -x \"npm run build\"",
    "translate": "babel ./JSX/PageApp.jsx -o ./public/PageApp.js",
    "pretranspile": "npm run translate",
    "transpile": "babel ./JSX/SetTexting.jsx -o ./public/SetTexting.js",
    "precompile": "npm run transpile",
    "compile": "babel ./JSX/View.jsx -o ./public/View.js",
    "watch": "watchify ./public/browser.js -o ./public/bundle.js -v",
    "start": "node server.js",
    "devstart": "npm run watch & nodemon server.js --ignore '/bundle.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Christopher Ganey",
  "license": "ISC",
